{
    "name": "nytodev/inertia-symfony",
    "description": "The Symfony adapter for Inertia.js",
    "type": "symfony-bundle",
    "require": {
        "php" : "^8.1",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "twig/twig": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytodev\\InertiaSymfony\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nytodev",
            "email": "tonyblard55@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "2.1.x-dev",
        "phpstan/phpstan-symfony": "2.0.x-dev",
        "squizlabs/php_codesniffer": "4.x-dev"
    }
}
